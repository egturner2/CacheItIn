{
  "manifest_version": 2,

  "name": "Cache It In",
  "description": "This extension will download all open tabs",
  "version": "1.0",
  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "downloads", "tabs", "<all_urls>", "pageCapture"
   ]
}